                      "chance": { "type": "number", "description": "UNDOCUMANTED", "title": "UNDOCUMANTED" },
                      "duration": { "type": "integer", "description": "UNDOCUMANTED", "title": "UNDOCUMANTED" },
                      "amplifier": { "type": "integer", "description": "UNDOCUMANTED", "title": "UNDOCUMANTED" }
                    }
                  }
