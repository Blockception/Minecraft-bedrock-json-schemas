                "remove_effects": {
                  "type": "array",
                  "description": "Can always eat this item? true or false.",
                  "title": "Remove Effects",
                  "items": { "type": "string", "description": "UNDOCUMANTED", "title": "UNDOCUMANTED" }
                }
