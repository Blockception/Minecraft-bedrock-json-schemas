        {
          "label": "New All_of Test",
          "body": {
            "all_of": [
              { "test": "$1", "value": "$2" },
              { "test": "$3", "value": "$4" }
            ]
          }
        },
        {
          "label": "New Any_of Test",
          "body": {
            "any_of": [
              { "test": "$1", "value": "$2" },
              { "test": "$3", "value": "$4" }
            ]
          }
        },
